nst onChange = e => {\n        const { name, value } = e.target;\n\n        setData(prevData => {\n            return {\n                ...prevData,\n                [name]: value\n            }\n        });\n        console.log(data);\n    };\n\n    const onChangeImage = async (e) => {\n        const file = e.target.files[0];\n        const storageRef = app.storage().ref();\n        const fileRef = storageRef.child(file.name);\n        await fileRef.put(file);\n        setPhoto(await fileRef.getDownloadURL());\n    }\n\n    const onSubmit = async e => {\n        e.preventDefault();\n\n        try {\n            const res = await axios.post('https://esg-metric-api.herokuapp.com/api/comp/', {\n                name: data.name,\n                industry: data.industry,\n                website: data.website,\n                total_emissions: data.emissions,\n                photo: photo,\n                description: data.description\n            });\n\n            console.log(res.data);\n\n            setData({\n                name: '',\n                industry: '',\n                website: '',\n                emissions: 0,\n            });\n\n            setImage('');\n        } catch (err) {\n        }\n    };\n\n    return (\n        <Fragment>\n            <div>\n                <Link to=\"/\">Back to Browse</Link>\n                <form onSubmit={onSubmit}>\n                    <label>\n                        Company Name:\n                        <input type=\"text\" name=\"name\" onChange={onChange} />\n                    </label>\n                    <label>\n                        Industry:\n                        <input type=\"text\" name=\"industry\" onChange={onChange} />\n                    </label>\n                    <label>\n                        Website:\n                        <input type=\"text\" name=\"website\" onChange={onChange} />\n                    </label>\n                    <label>\n                        Company Logo:\n  